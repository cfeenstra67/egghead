{
  "name": "Egghead History",
  "manifest_version": 2,
  "version": "1.3",
  "description": "Browser history replacement that doesn't suck",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "alarms",
    "history"
  ],
  "chrome_url_overrides": {
    "history": "history.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "MacCtrl+Shift+H"
      }
    }
  }
}
