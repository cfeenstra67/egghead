{
  "name": "Browser History",
  "manifest_version": 2,
  "version": "1.3",
  "description": "Browser History Replacement",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "chrome_url_overrides": {
    "history": "history.html"
  },
  "cross_origin_embedder_policy": {
    "value": "require-corp"
  },
  "cross_origin_opener_policy": {
    "value": "same-origin"
  }
}
