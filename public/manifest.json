{
  "name": "Egghead",
  "manifest_version": 3,
  "version": "0.0.1",
  "description": "Egghead history",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "alarms"
  ],
  "chrome_url_overrides": {
    "history": "history.html"
  }
}
