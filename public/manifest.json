{
  "name": "Browser History",
  "manifest_version": 2,
  "version": "1.3",
  "description": "Browser History Replacement",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "alarms"
  ],
  "chrome_url_overrides": {
    "history": "history.html"
  }
}
